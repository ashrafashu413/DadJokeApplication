{
  "DadJokeSettings": {
    "BaseUrl": "https://icanhazdadjoke.com/",
    "UserAgent": "DadJokeApi/1.0 (+https://yourdomain.com)",
    "CacheDurationMinutes": 30,
    "SearchLimit": 30
  },  
    "ElasticSearch": {
      "Uri": "http://localhost:9200",
      "DefaultIndex": "jokes"
    }, 

  "ConnectionStrings": {
    "DefaultConnection": "UseDevelopmentDatabaseInLocalOnly"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
